---
outputs:
    EsDomain:
        Description:
            Physical ID of the ES domain '{{domain_name}}'.
        Value:
            Ref: EsDomain
    EsDomainEndpoint:
        Description:
            Endpoint for the ES domain '{{domain_name}}'.
        Value:
            "Fn::GetAtt":
                - EsDomain
                - DomainEndpoint
    EsDomainArn:
        Description:
            Arn for the ES domain '{{domain_name}}'.
        Value:
            "Fn::GetAtt":
                - EsDomain
                - DomainArn
