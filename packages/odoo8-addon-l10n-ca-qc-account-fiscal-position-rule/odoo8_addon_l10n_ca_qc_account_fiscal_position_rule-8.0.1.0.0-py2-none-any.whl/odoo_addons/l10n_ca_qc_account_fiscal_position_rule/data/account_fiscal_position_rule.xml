        <record model="account.fiscal.position.rule" id="rule_fr_qc2qc">
            <field name="name">From Quebec to Quebec</field>
            <field name="description">From a company in Quebec to a partner in Quebec</field>
            <field name="company_id">1</field>
            <field name="fiscal_position_id" model="account.fiscal.position" search="[('name','like','%(QC)%')]"/>
            <field name="from_country" model="res.country" search="[('code','=','CA')]"/>
            <field name="from_state" model="res.country.state" search="[('code','=','QC')]"/>
