            <field name="to_shipping_state" model="res.country.state" search="[('code','=','QC')]"/>
            <field name="use_invoice">TRUE</field>
            <field name="use_picking">TRUE</field>
            <field name="use_purchase">TRUE</field>
            <field name="use_sale">TRUE</field>
        </record>
        
