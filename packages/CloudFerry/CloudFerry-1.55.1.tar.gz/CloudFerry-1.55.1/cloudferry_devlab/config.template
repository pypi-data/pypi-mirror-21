factor = 0.1
test_file_size = 10

[condense]
group_file=

[database]
host = localhost
port = 6379

[bbcp]
path = <bbcp_path>
options = -P 5 -e

[rsync]
port = 50000

[rollback]
keep_migrated_vms = True
