<?xml version="1.0"?>
<form title="ADB GUI" width='800' height='600'>
<menubar>
	<menu title='Device'>
        <menu id='id_adb_devices' title='adb devices'/>
        <menu id='id_list_package' title='list package'/>
        <menu id='id_input_keyevent' title='Input KeyEvent'/>
        <menu id='id_screen_shot' title='Screen Shot'/>
        <menu id='id_screen_record' title='Screen Record'/>
        <menu id='id_reboot' title='Reboot'/>
        <menu id='id_adb_pull' title='Adb Pull'/>
        <menu id='id_adb_push' title='Adb Push'/>
        <menu id='id_setting' title='Setting'/>
	</menu>
	<menu title='APK'>
        <menu id='id_list_sys_package' title='list system package'/>
        <menu id='id_list_3rd_package' title='list 3rd package'/>
        <menu id='id_install_app' title='Install App'/>
        <menu id='id_uninstall_app' title='Uninstall App'/>
        <menu id='id_clear_data' title='Clear App Data'/>
        <menu id='id_stop_app' title='Stop App'/>
	</menu>
	<menu title='Dump'>
        <menu id='id_dump' title='Dump'/>
        <menu id='id_dump_activities' title='Dump Activities'/>
        <menu id='id_dump_services' title='Dump Services'/>
        <menu id='id_dump_display' title='Dump Display'/>
        <menu id='id_current_activity' title='Current Activity'/>
	</menu>
	<menu title='Hardware'>
        <menu id='id_info_product' title='Product'/>
        <menu id='id_dump_battery' title='Battery'/>
        <menu id='id_info_screen_size' title='Screen Size'/>
        <menu id='id_info_screen_density' title='Screen Density'/>
        <menu id='id_info_android_id' title='Android Id'/>
    </menu>
	<menu title='Log'>
        <menu id='id_kernel_log' title='Kernel Log'/>
        <menu id='id_logcat' title='Log Cat'/>
	</menu>
	<menu title='Info'>
        <menu id='id_info_cpu' title='Cpu'/>
        <menu id='id_info_memory' title='Memory'/>
        <menu id='id_info_imei' title='IMEI'/>
        <menu id='id_info_ps' title='Process Info'/>
        <menu id='id_info_top' title='Top'/>
        <menu id='id_info_network' title='Network'/>
        <menu id='id_info_wifi_passwd' title='Wifi Password'/>
	</menu>
</menubar>
<panel name='' id="pid_install_app" visible="false" height="350">
        <line>
            <file id="id_install_apk_file" title="Apk File" mark="*.apk"/>
        </line>
        <line>
            <check id="id_install_sdcard" title="Install to SDCard"/>
        </line>
     <line>
            <check id="id_install_grand_permission" title="Grand Permission"/>
     </line>
    <line>
        <button id="id_button_install_apk" title="Install"/>
     </line>
</panel>
<panel name='' id="pid_input_keyevent" visible="false" height="350">
        <line>
            <choise id="id_keyevent" value="power"/>
        </line>
    <line>
        <button id="id_button_input_keyevent" title="Input Key"/>
     </line>
</panel>
    <panel name='' id="pid_reboot" visible="false" height="350">
        <line>
            <choise id="id_reboot_type" choices="normal;recovery;bootloader;recovery"/>
        </line>
    <line>
        <button id="id_reboot_button" title="Reboot"/>
     </line>
</panel>
    <panel name='' id="pid_dump" visible="false" height="550">
        <line expand="true">
            <static title="Service:" width="100"/>
            <check_list id='id_service' choices=""  value=""/>
        </line>
    <line>
        <button id="id_dump_service" title="Dump"/>
     </line>
</panel>
<panel name='' id="pid_logcat" visible="false" height="350">
        <line>
            <static title="Priority" width="100" />
            <choise id="id_logcat_priority" choices="Verbose;Debug;Info;Warning;Error;Fatal;Silent" value = 'Verbose'/>
        </line>
        <line>
            <static title="Format" width="100" />
            <choise id="id_logcat_format" choices=";brief;process;tag;raw;time;threadtime;long" value = ''/>
        </line>
        <line>
            <static title="Grep" width="100" />
            <text id="id_logcat_grep"/>
        </line>
    <line>
        <button id="id_logcat_button" title="LogCat"/>
     </line>
</panel>
<panel name='' id="pid_adb_pull" visible="false" height="350">
        <line>
            <static title="File on Device:" width="150" />
            <text id="id_adb_pull_file" value = ''/>
            <button id = 'id_adb_pull_file_btn' title="Select File" width="150" />
        </line>
        <line>
            <folder id='id_adb_pull_save' title='Save To:' value="~"/>
        </line>
    <line>
        <button id="id_adb_pull_button" title="Adb Pull"/>
     </line>
</panel>
<panel name='' id="pid_adb_push" visible="false" height="350">
        <line>
            <file id='id_adb_push_file' title='File:' value=""/>
        </line>
        <line>
            <static title="Folder on Device:" width="150" />
            <text id="id_adb_push_folder" value = ''/>
            <button id = 'id_adb_push_folder_btn' title="Select Path" width="150" />
        </line>
    <line>
        <button id="id_adb_push_button" title="Adb Push"/>
     </line>
</panel>
    <panel name='' id="pid_setting" visible="false" height="350">
        <line>
            <file id='id_setting_adb_path' title='File:' value="adb"/>
        </line>
    <line>
        <button id="id_setting_btn" title="OK"/>
     </line>
</panel>
</form>