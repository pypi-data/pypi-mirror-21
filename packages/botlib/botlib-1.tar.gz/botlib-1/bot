#!/usr/bin/env python3
#
#

""" Die medicijnen zijn gif en het toedienen ervan is stafbaar !! """

from botlib.utils import parse_cli, reset, startup
from botlib.space import kernel

import os

startup()

def main():
    cfg = parse_cli()
    kernel.boot(cfg)
    kernel.wait()
    kernel.shutdown()

if __name__ == "__main__":
    try:
        main()
    except KeyboardInterrupt:
        print("")
    reset()
    os._exit(0)
