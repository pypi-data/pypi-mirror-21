{
    "strictNullChecks": true,
    "allowUnreachableCode": true,
    "compilerOptions": {
        "declaration": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noEmitOnError": true,
        "moduleResolution": "node",
        "baseUrl": "",
        "module": "commonjs",
        "target": "es5",
        "typeRoots": [
            "./node_modules/@types"
        ],
        "types": [
            "moment"
        ]
    },
    "exclude": [
        "node_modules/@types/node",
        "node_modules",
        "platforms",
        "e2e",
        "src",
        "**/*spec.ts",
        "**/main.web.ts",
        "**/app.web.module.ts",
        "**/main.aot.ts"
    ]
}